---
layout: null
permalink: /search.json
---
[
  {% assign sorted = site.notes | sort: "last_modified_at" | reverse %}
  {% for note in sorted %}
  {
    "title": {{ note.title | jsonify }},
    "url": "{{ site.baseurl }}{{ note.url }}",
    "excerpt": {{ note.content | strip_html | strip_newlines | truncatewords: 30 | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
